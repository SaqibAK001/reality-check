      "id": "07d8deee-bc1c-4d87-a8f1-b416fa906a31",
      "name": "AI Image Detection",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 3,
      "position": [
        2288,
        544
      ]
    },
    {
      "parameters": {
        "functionCode": "const items = $input.all();\nconst results = [];\n\nfor (let i = 0; i < items.length; i++) {\n  const original = items[i].json;\n  const detection = items[items.length - 1].json; // Last item has detection results\n  \n  const isImageDetection = original.media_type === 'image';\n  \n  // Extract confidence score\n  const confidence = detection.confidence_score || detection.confidence || 0;\n  const isFake = detection.is_ai_generated || detection.is_deepfake || false;\n  \n  // Risk classification\n  let riskLevel, riskEmoji, riskColor;\n  if (confidence >= 0.8) {\n    riskLevel = 'HIGH';\n    riskEmoji = 'üö®';\n    riskColor = '#FF0000';\n  } else if (confidence >= 0.6) {\n    riskLevel = 'MEDIUM';\n    riskEmoji = '‚ö†Ô∏è';\n    riskColor = '#FFA500';\n  } else {\n    riskLevel = 'LOW';\n    riskEmoji = '‚úÖ';\n    riskColor = '#00FF00';\n  }\n  \n  // Get indicators\n  const indicators = detection.indicators || detection.manipulation_types_detected || [];\n  \n  results.push({\n    json: {\n      // Original context\n      platform: original.platform,\n      user_id: original.user_id,\n      chat_id: original.chat_id,\n      username: original.username,\n      processing_id: original.processing_id,\n      message_id: original.message_id,\n      \n      // Detection results\n      media_type: original.media_type,\n      detection_type: isImageDetection ? 'AI Generation' : 'Deepfake',\n      is_fake: isFake,\n      confidence_score: confidence,\n      confidence_percentage: `${(confidence * 100).toFixed(1)}%`,\n      \n      // Risk assessment\n      risk_level: riskLevel,\n      risk_emoji: riskEmoji,\n      risk_color: riskColor,\n      requires_alert: confidence >= 0.75,\n      \n      // Technical details\n      indicators: indicators,\n      model_version: detection.model_version || 'unknown',\n      processing_time: detection.processing_time_ms || 0,\n      \n      // Timestamp\n      analyzed_at: new Date().toISOString()\n    },\n    binary: items[i].binary\n  });\n}\n\nreturn results;"
      },
      "id": "2dc8f796-6b43-4882-b500-93c1fbcce6c0",
