    {
      "parameters": {
        "functionCode": "const platformData = $input.all();\nconst normalized = [];\n\nfor (const item of platformData) {\n  const data = item.json;\n  let standardized = {};\n\n  // Telegram normalization\n  if (data.message && data.message.chat) {\n    const msg = data.message;\n    const hasPhoto = msg.photo && msg.photo.length > 0;\n    const hasVideo = msg.video;\n    \n    standardized = {\n      platform: 'telegram',\n      user_id: msg.from.id.toString(),\n      chat_id: msg.chat.id.toString(),\n      media_type: hasPhoto ? 'image' : (hasVideo ? 'video' : 'unknown'),\n      media_identifier: hasPhoto ? msg.photo[msg.photo.length - 1].file_id : (hasVideo ? msg.video.file_id : null),\n      username: msg.from.username || msg.from.first_name || 'Unknown',\n      timestamp: msg.date,\n      download_method: 'telegram_getFile',\n      message_id: msg.message_id\n    };\n  }\n  // WhatsApp normalization\n  else if (data.entry && data.entry[0] && data.entry[0].changes) {\n    const change = data.entry[0].changes[0].value;\n    const message = change.messages ? change.messages[0] : null;\n    \n    if (message) {\n      standardized = {\n        platform: 'whatsapp',\n        user_id: message.from,\n        chat_id: message.from,\n        media_type: message.type === 'image' ? 'image' : (message.type === 'video' ? 'video' : 'unknown'),\n        media_identifier: message.image ? message.image.id : (message.video ? message.video.id : null),\n        username: message.from,\n        timestamp: message.timestamp,\n        download_method: 'whatsapp_getMedia',\n        message_id: message.id\n      };\n    }\n  }\n  // Discord normalization\n  else if (data.author && data.attachments) {\n    const attachment = data.attachments[0];\n    const isImage = attachment.content_type && attachment.content_type.includes('image');\n    const isVideo = attachment.content_type && attachment.content_type.includes('video');\n    \n    standardized = {\n      platform: 'discord',\n      user_id: data.author.id,\n      chat_id: data.channel_id,\n      media_type: isImage ? 'image' : (isVideo ? 'video' : 'unknown'),\n      media_identifier: attachment.url,\n      username: data.author.username,\n      timestamp: data.timestamp,\n      download_method: 'direct_url',\n      message_id: data.id\n    };\n  }\n\n  // Add universal metadata\n  if (standardized.platform) {\n    standardized.processing_id = `${standardized.platform}_${Date.now()}_${Math.random().toString(36).substr(2, 9)}`;\n    standardized.status = 'pending';\n    standardized.received_at = new Date().toISOString();\n    \n    normalized.push({ json: standardized });\n  }\n}\n\nreturn normalized;"
      },
